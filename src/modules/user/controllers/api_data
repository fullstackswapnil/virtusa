// us cities and state 
https://raw.githubusercontent.com/kelvins/US-Cities-Database/master/us_cities.sql

Register Hospital
url : localhost:5001/api/user/registerHospital

Request body
{
  
      "email" : "swapnilsakha2@gmail.com",
      "password": "123456789",
      "referral_code_id" : "121211"
    
}

Response body
{
    "status": "success",
    "status_code": 200,
    "message": "Email registered successfully.",
    "data": {
        "success": true,
        "data": [
            {
                "email": "swapnilsakha2@gmail.com",
                "password": "123456789",
                "referral_code_id": "121211",
                "user_id": 7,
                "hospital_id": 2
            }
        ]
    }
}

Register Vet 
url : localhost:5001/api/user/registerVet
{
    "status": "success",
