imp sql 

 `SELECT  json_object(
                    'state_id', 
                    state.`id`, 
                    'state_name',
                    state.`state_name`, 
                     
                    
                    'cities', 
                    
                    (select cast(concat('[',
                                    GROUP_CONCAT( json_object( 'city_id',id, 
                                                                'city_name', `city`
                                                                )),
                                    ']')  as JSON)
                    FROM city 
                    WHERE state.id = city.state_id)) as json_definition 
                    FROM state`;
select json_object(
  'state_id',state.id ,
  'cities',(select CAST(CONCAT('[',
                GROUP_CONCAT(
                  JSON_OBJECT(
                    'city_id',id )),
                ']')
         AS JSON) from city where state_id = state.id)

 ) from state p;



 // media type
 // roll_id



